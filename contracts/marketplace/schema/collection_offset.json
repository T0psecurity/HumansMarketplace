{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CollectionOffset",
  "description": "Offset for collection pagination",
  "type": "object",
  "required": [
    "collection",
    "token_id"
  ],
  "properties": {
    "collection": {
      "type": "string"
    },
    "token_id": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
