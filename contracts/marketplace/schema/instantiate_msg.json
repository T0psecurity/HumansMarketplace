{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "ask_expiry",
    "bid_expiry",
    "operators",
    "trading_fee"
  ],
  "properties": {
    "ask_expiry": {
      "description": "Valid time range for Asks (min, max) in seconds",
      "type": "array",
      "items": [
        {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      ],
      "maxItems": 2,
      "minItems": 2
    },
    "bid_expiry": {
      "description": "Valid time range for Bids (min, max) in seconds",
      "type": "array",
      "items": [
        {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      ],
      "maxItems": 2,
      "minItems": 2
    },
    "operators": {
      "description": "Operators are entites that are responsible for maintaining the active state of Asks. They listen to NFT transfer events, and update the active state of Asks.",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "sales_finalized_hook": {
      "type": [
        "string",
        "null"
      ]
    },
    "trading_fee": {
      "description": "Fair Burn fee for winning bids i.e. 125 = 0.125%, 250 = 0.25%, 500 = 0.5%, 1000 = 1%, 2500 = 2.5%",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
