{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "admins",
    "admins_mutable",
    "max_expiry",
    "min_expiry",
    "trading_fee_percent"
  ],
  "properties": {
    "admins": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "admins_mutable": {
      "type": "boolean"
    },
    "max_expiry": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "min_expiry": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "trading_fee_percent": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
