{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "max_expiry",
    "min_expiry",
    "operators",
    "trading_fee_percent"
  ],
  "properties": {
    "max_expiry": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "min_expiry": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "operators": {
      "description": "Operators are entites that are responsible for maintaining the active state of Asks. They listen to NFT transfer events, and update the active state of Asks.",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "trading_fee_percent": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
